{
  "name": "electron-vue3-vite-template",
  "version": "2.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "ame",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "serve": "pnpm run dev",
    "dev": "vite --mode development",
    "test": "vite --mode test",
    "build": "vue-tsc --noEmit && vite build --mode production && electron-builder",
    "preview": "vite preview",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint --ext .vue --ext .js --ext .ts src/ --fix",
    "style": "stylelint \"src/**/*.(vue|scss|css)\" --fix",
    "postinstall": "husky install",
    "cz": "git add . && cz"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@unocss/preset-icons": "^0.51.8",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "auto-import-types": "^0.0.4",
    "autoprefixer": "^10.4.14",
    "comment-json": "^4.2.3",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.1",
    "cz-customizable": "^7.0.0",
    "electron": "^23.3.1",
    "electron-builder": "^23.6.0",
    "electron-updater": "^5.3.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "pinia-auto-refs": "^0.0.5",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.1",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "^5.0.4",
    "unocss": "^0.51.8",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.4",
    "vite-plugin-commonjs": "^0.7.0",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.1",
    "vue": "^3.2.47",
    "vue-global-api": "^0.4.1",
    "vue-tsc": "^1.6.4"
  },
  "dependencies": {
    "@iconify-json/ep": "^1.1.10",
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "electron-log": "^4.4.8",
    "element-plus": "^2.3.4",
    "filepond": "^4.30.4",
    "filepond-plugin-file-validate-size": "^2.2.8",
    "filepond-plugin-file-validate-type": "^1.2.8",
    "filepond-plugin-image-edit": "^1.6.3",
    "filepond-plugin-image-preview": "^4.6.11",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isobject": "^3.0.2",
    "pinia": "^2.0.35",
    "pinia-plugin-persistedstate": "^3.1.0",
    "vue-filepond": "^7.0.3",
    "vue-router": "^4.1.6"
  }
}